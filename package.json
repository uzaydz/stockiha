{
  "name": "stockiha-console-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite --config vite.config.desktop.ts",
    "dev:simple": "vite --config vite.config.dev.ts --force --host localhost --port 8080",
    "dev:fast": "vite --force --host 0.0.0.0 --port 8080",
    "dev:optimized": "VITE_OPTIMIZE=true vite --force",
    "dev:minimal": "VITE_MINIMAL_PROVIDERS=true vite --force",
    "dev:fast-refresh": "FAST_REFRESH=true vite --force --host 0.0.0.0 --port 8080",
    "dev:no-strict": "VITE_DISABLE_STRICT_MODE=true vite --force",
    "build": "vite build --config vite.config.desktop.ts",
    "build:optimized": "npm run build && node fix-cache-headers.js",
    "build:ultra": "vite build",
    "build:code-splitting": "vite build --config vite.config.code-splitting.ts",
    "build:cloudflare": "VITE_DEPLOYMENT_PLATFORM=cloudflare vite build && wrangler pages deploy dist",
    "build:store": "VITE_BUILD_TARGET=store vite build",
    "dev:store": "VITE_BUILD_TARGET=store vite --host 0.0.0.0 --port 8080",
    "deploy:cloudflare": "pnpm run build:cloudflare",
    "deploy:cloudflare:preview": "VITE_DEPLOYMENT_PLATFORM=cloudflare vite build && wrangler pages deploy dist --project-name stockiha-preview",
    "lint": "eslint .",
    "lint:css": "stylelint \"src/**/*.{css,scss}\" --fix",
    "lint:css:check": "stylelint \"src/**/*.{css,scss}\"",
    "preview": "vite preview",
    "preview:cloudflare": "wrangler pages dev dist",
    "api": "node --experimental-modules --es-module-specifier-resolution=node src/server/index.js",
    "dev:with-api": "concurrently -k \"npm run dev\" \"npm run api\"",
    "test:mobile": "node scripts/test-mobile-connection.js",
    "dev:mobile": "npm run dev && npm run test:mobile",
    "optimize": "node scripts/performance-optimizer.js",
    "css:split": "node scripts/css-splitter.js",
    "analyze": "node scripts/analyze-bundles.js",
    "analyze:optimized": "node scripts/analyze-bundle-optimized.js",
    "build:analyze": "npm run build && npm run analyze",
    "build:analyze:optimized": "npm run build:code-splitting && npm run analyze:optimized",
    "analyze:detailed": "npm run build && npx vite-bundle-analyzer dist",
    "perf:test": "node scripts/apply-optimizations.js",
    "perf:analyze": "node scripts/performance-analysis.js",
    "measure:http": "node scripts/measure-http-requests.js",
    "test:f24": "npm run measure:http",
    "split:database": "node scripts/split-database-types.js",
    "split:database-improved": "node scripts/split-database-types-improved.js",
    "update:imports": "node scripts/update-database-imports.js",
    "optimize:types": "npm run split:database-improved",
    "dev:virt": "VITE_VIRTUALIZE_ORDERS=true vite",
    "dev:inf": "VITE_INFINITE_SCROLL=true vite",
    "dev:virt-inf": "VITE_VIRTUALIZE_ORDERS=true VITE_INFINITE_SCROLL=true vite",
    "build:critical": "npm run build && node scripts/generate-critical-css.cjs",
    "deploy:cloudflare:optimized": "npm run build:optimized && wrangler pages deploy dist",
    "optimize:build": "node scripts/build-optimization.js",
    "setup:cloudflare": "node cloudflare-setup.js",
    "build:production": "npm run build:ultra && npm run optimize:build && npm run optimize:images",
    "deploy:production": "npm run build:production && wrangler pages deploy dist",
    "images:gen": "node scripts/generate-responsive-images.mjs",
    "perf:optimize": "node scripts/performance-optimization.js",
    "perf:critical": "npm run build:critical && npm run perf:optimize",
    "desktop:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:8080 && ELECTRON_IS_DEV=true electron . --dev\"",
    "desktop:build": "vite build --config vite.config.desktop.ts",
    "desktop:build:mac": "npm run desktop:build && electron-builder --mac --x64 --arm64",
    "desktop:pack": "npm run desktop:build && electron-builder",
    "desktop:dist": "npm run desktop:build && electron-builder --publish=never",
    "desktop:dist-mac": "npm run desktop:build && electron-builder --mac",
    "desktop:dist-win": "npm run desktop:build && electron-builder --win",
    "desktop:dist-linux": "npm run desktop:build && electron-builder --linux",
    "desktop:install": "npm install electron electron-builder concurrently wait-on --save-dev"
  },
