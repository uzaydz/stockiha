{
  "name": "stockiha-console-desktop",
  "private": true,
  "version": "1.0.0",
  "main": "electron.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "wait-on tcp:8080 && electron .",
    "electron:direct": "electron .",
    "start": "npm run build && npm run electron:direct",
    "start:electron": "npm run build && npm run electron:direct",
    "electron:dev": "concurrently -k \"npm run dev\" \"npm run electron\"",
    "electron:build": "npm run build && electron-builder",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:win": "npm run build && electron-builder --win",
    "build:desktop": "node build-desktop.js",
    "api": "node --experimental-modules --es-module-specifier-resolution=node src/server/index.js",
    "dev:with-api": "concurrently -k \"npm run dev\" \"npm run api\""
  },
  "build": {
    "appId": "com.stockiha.console",
    "productName": "Stockiha Console",
    "files": [
      "dist/**/*",
      "electron.cjs",
      "preload.cjs",
      "main.cjs",
      "electron-init.cjs",
      "logs/**/*"
    ],
    "directories": {
      "output": "dist_electron",
      "buildResources": "public"
    },
    "asar": true,
    "mac": {
      "category": "public.app-category.business",
      "target": [
        "dmg"
      ],
      "icon": "public/favicon.ico",
      "darkModeSupport": true,
      "hardenedRuntime": true
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "dmg": {
      "icon": "public/favicon.ico",
      "artifactName": "StockihaConsole-${version}.${ext}",
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
